/* Fonts */
@font-face {
    font-family: 'aileronbold';
    src: url('/static/fonts/aileron-bold-webfont.eot');
    src: url('/static/fonts/aileron-bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('/static/fonts/aileron-bold-webfont.woff') format('woff'),
         url('/static/fonts/aileron-bold-webfont.ttf') format('truetype'),
         url('/static/fonts/aileron-bold-webfont.svg#aileronbold') format('svg');
    font-weight: normal;
    font-style: normal;

}
